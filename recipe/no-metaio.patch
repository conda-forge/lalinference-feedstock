-esac
-         ;; #(
-  -I*) :
-
-          case " ${AM_CPPFLAGS} " in #(
-  *" ${flag} "*) :
-    : ;; #(
-  *) :
-    pre_AM_CPPFLAGS="${pre_AM_CPPFLAGS} ${flag}"
