-  -I/opt/*|-I/usr/*) :
-
-          case " ${sys_CPPFLAGS} " in #(
-  *" ${flag} "*) :
-    : ;; #(
-  *) :
-    pre_sys_CPPFLAGS="${pre_sys_CPPFLAGS} ${flag}"
